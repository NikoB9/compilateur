function printx(a)
{
    if(a != 0)
    {
        printx(a/10);
        send a%10;
    }
}

/*
* Permet de réaliser des affichages dans le programme
*/
function print(a)
{
    //Si a vaut zero cela correspond au code ascii 40
    if(a==0) send 40;
    else printx(a);
    send 10;
}